{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@config/*": ["config/*"],
      "@data/*": ["data/*"],
      "@enums/*": ["enums/*"],
      "@hoc/*": ["hoc/*"],
      "@hooks/*": ["hooks/*"],
      "@images/*": ["images/*"],
      "@interfaces/*": ["interfaces/*"],
      "@pages/*": ["pages/*"],
      "@public/*": ["public/*"],
      "@store/*": ["store/*"],
      "@utils/*": ["utils/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
